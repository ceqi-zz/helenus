package com.ceqi.footballBettingRecommendation.shared;

import java.io.Serializable;

public class ScoreWithPos implements Serializable {
	private double score;
	private int row;
	private int col;

	public ScoreWithPos() {

	}

	public ScoreWithPos(double score, int row, int col) {
		this.score = score;
		this.row = row;
		this.col = col;
	}

	public double getScore() {
		return score;
	}

	public int getRow() {
		return row;
	}

	public int getCol() {
		return col;
	}

}
